    class Program
    {
        static void Main(string[] args)
        {
            //on specifie le chemin du fichier text afin de le lire et ecrire la-dedans
            string cheminDuFichierText = @"C:\Users\Houda\Desktop\Assurance_Qualit√©_Logicielle\DossierEtudiant.txt";
            
